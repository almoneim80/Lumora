  },
  "EmailVerificationApi": {
    "Url": "https://emailverification.whoisxmlapi.com/api/v2",
    "ApiKey": ""
  },
  "AccountDetailsApi": {
    "Url": "https://www.klazify.com/api/categorize",
    "ApiKey": ""
  },
  "Cookies": {
    "Enable": true,
    "Name": "auth_ticket",
    "ExpireTime": "12"
  },
  "Jwt": {
    "Secret": "",
    "Issuer": "",
    "Audience": "",
    "RefreshTokenExpirationDays": 30,
    "AccessTokenExpirationMinutes": 15
  },
  "Identity": {
    "LockoutTime": "5",
    "MaxFailedAccessAttempts": "10"
  },
  "LocalizationOptions": [
    {
      "Name": "العربية",
      "Value": "ar"
    },
    {
      "Name": "English",
      "Value": "en-US"
    }
  ],
  "PayTabs": {
    "ServerKey": "",
    "ProfileId": "164138",
