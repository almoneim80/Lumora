      "Password": "Pass@123!",
      "IsActive": true,
      "PhoneNumberConfirmed": true,
      "EmailConfirmed": true,
      "Roles": [ "SuperAdmin" ]
    }
  ],
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "Postgres": {
    "Server": "localhost",
    "Port": "5432",
    "UserName": "postgres",
    "Password": "pass",
    "Database": "LumoraDb"
  },
  "Elastic": {
    "Server": "localhost",
    "Port": "9200",
    "UseHttps": false,
    "UserName": "elastic",
    "Password": "",
    "IndexPrefix": "Lumora"
  },
  "Email": {
    "Server": "smtp.elasticemail.com",
    "UserName": "test@gmail.com",
    "Password": "",
    "Port": "2525",
    "UseSsl": "false"
  },
  "EmailSender": {
    "FromEmail": "test@gmail.com",
    "FromName": "Lumora"
  },
  "OpenAI": {
    "ApiKey": ""
  },
  "TaskRunner": {
    "CronSchedule": "0/30 * * * * ?"
  },
  "Tasks": {
    "DomainVerificationTask": {
      "Enable": true,
      "CronSchedule": "0/30 * * * * ?",
      "RetryCount": "2",
      "RetryInterval": "1",
      "BatchSize": "1",
      "BatchInterval": 1
    },
    "HardDeleteTask": {
      "Enable": true,
      "CronSchedule": "0 01 02 * * ?",
      "RetryCount": 3,
      "RetryInterval": 5,
      "HardDeleteBatchSize": 200
    },
    "SyncProgramProgressTask": {
      "Enable": true,
      "CronSchedule": "0 0 2 * * ?",
      "ProgramId": 5,
      "RetryCount": 2,
      "RetryInterval": 3
    },
    "SyncEsTask": {
      "Enable": true,
      "CronSchedule": "0/30 * * * * ? *",
      "RetryCount": "2",
      "RetryInterval": "5",
      "BatchSize": "500"
    },
    "ContactScheduledEmail": {
      "Enable": false,
      "CronSchedule": "0 0/1 * * * ?",
      "RetryCount": "2",
      "RetryInterval": "5"
    },
    "SyncEmailLogTask": {
      "Enable": false,
      "CronSchedule": "0/30 * * * * ? *",
      "RetryCount": "2",
      "RetryInterval": "5",
      "BatchSize": "100"
    },
    "SyncIpDetailsTask": {
      "Enable": false,
      "CronSchedule": "0 0/1 * * * ?",
      "RetryCount": "2",
      "RetryInterval": "5",
      "BatchSize": "50"
    },
    "SyncSmsLogTask": {
      "Enable": false,
      "CronSchedule": "0/30 * * * * ? *",
      "RetryCount": "2",
      "RetryInterval": "5",
      "BatchSize": "100"
    }
  },
  "AppSettings": {
    "BaseUrl": "https://localhost:45437"
  },
  "FileUploadSettings": {
    "Videos": {
      "Extensions": [ ".avi", ".mov", ".mkv", ".flv", ".wmv", ".webm", ".mp4" ],
      "MimeTypes": [ "video/avi", "video/quicktime", "video/x-matroska", "video/x-flv", "video/x-ms-wmv", "video/webm", "video/mp4" ],
      "MaxSizePerExtension": {
        "default": "500MB",
        ".mp4": "1GB",
        ".webm": "1GB",
        ".mkv": "700MB"
      }
    },
    "Documents": {
      "Extensions": [ ".pdf", ".xlsx", ".pptx", ".ppt", ".ppsx", ".doc", ".docx" ],
      "MimeTypes": [
        "application/pdf",
        "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet", // .xlsx
        "application/vnd.openxmlformats-officedocument.presentationml.presentation", // .pptx
        "application/vnd.ms-powerpoint", // .ppt, .ppsx
        "application/msword", // .doc
        "application/vnd.openxmlformats-officedocument.wordprocessingml.document" // .docx
      ],
      "MaxSizePerExtension": {
        "default": "10MB",
        ".pdf": "15MB",
        ".pptx": "20MB"
      }
    },
    "Images": {
      "Extensions": [ ".png", ".jpg", ".jpeg", ".svg", ".tiff" ],
      "MimeTypes": [ "image/png", "image/jpeg", "image/svg+xml", "image/tiff" ],
      "MaxSizePerExtension": {
        "default": "5MB",
        ".tiff": "10MB"
      }
    }
  },
  "OtpVerification": {
    "IsInMemoryCache": true,
    "EnableUrl": true,
    "Iterations": 1,
    "Size": 6,
    "Length": 20,
    "Expire": 5,
    "BaseOtpUrl": "https://yourdomain.com/verify?code="
  },
  "Authentication": {
    "Google": {
      "ClientId": "",
      "ClientSecret": "",
      "RedirectUri": "http://localhost:45437/api/identity/callback"
    }
  },
  "CacheProfiles": [
    {
      "Type": "ImageResponse",
      "Duration": 1200,
      "VaryByHeader": "User-Agent"
    }
  ],
  "Cors": {
    "AllowedOrigins": [ "http://localhost:45437" ]
  },
  "GeolocationApi": {
    "Url": "https://api.ipgeolocation.io/ipgeo",
    "AuthKey": ""
  },
  "OtpOptions": {
    "ExpireInMinutes": 5,
    "ResendCooldownSeconds": 60,
    "SmsTemplate": "Your verification code is {otp}. It will expire in {minutes} minutes.",
    "MaxFailedAttempts": 5,
    "BlockDurationMinutes": 5,
    "FailedAttemptWindowMinutes": 10
  },
  "ApiSettings": {
    "MaxListSize": 20,
    "MaxImportSize": "5120MB",
    "DefaultLanguage": "ar"
  },
  "RefundSettings": {
    "MaxRefundDays": 7
  },
  "PayTabsSettings": {
    "ProfileId": "",
    "ServerKey": "",
    "BaseUrl": "https://secure-global.paytabs.com", // for global
    "ReturnUrl": "https://Lumora.com/payment/success",
    "CallbackUrl": "https://Lumora.com/payment/callback",

    "SWejhaportedCurrencies": [ "AED", "USD", "SAR" ],
