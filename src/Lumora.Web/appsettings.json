  "MigrateOnStart": true,
  "Plugins": [ "Lumora.Plugin.Sms" ],
  "Entities": {
    "Include": [],
    "Exclude": []
  },
  "CacheSettings": {
    "CacheExpirationMinutes": 10,
    "CacheItemProiority": "High",
    "CacheItemSize": 1024
  },
  "DefaultUsers": [
    {
      "FullName": "User One",
      "City": "Paris",
